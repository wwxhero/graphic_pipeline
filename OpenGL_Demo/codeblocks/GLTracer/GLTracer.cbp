				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add option="-fPIC" />
			<Add option="-DSYNC_LOGGING" />
		</Compiler>
		<Unit filename="../../GLTracer/libGLTracer.cpp" />
		<Unit filename="../../GLTracer/libGLTracer.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
